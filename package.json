{
  "name": "shopping_basket_api",
  "version": "1.0.0",
  "description": "A Shopping basket application that allows users to keep track of the things they want to buy",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "env-cmd -f ./config/.env nodemon server.js",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendistephen/shopping_basket_api.git"
  },
  "author": "Ssendikadiwa Stephen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sendistephen/shopping_basket_api/issues"
  },
  "homepage": "https://github.com/sendistephen/shopping_basket_api#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "env-cmd": "^9.0.2",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.12"
  }
}
